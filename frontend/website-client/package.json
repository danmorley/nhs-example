{
  "name": "website-client",
  "version": "0.11.21",
  "private": true,
  "dependencies": {
    "bootstrap-4-grid": "^2.3.0",
    "html-react-parser": "^0.4.3",
    "lodash.startswith": "^4.2.1",
    "node-sass-chokidar": "0.0.3",
    "normalize.css": "^7.0.0",
    "npm-run-all": "^4.1.2",
    "query-string": "^5.1.1",
    "react": "^16.3.1",
    "react-cookie": "^2.1.4",
    "react-document-meta": "^2.1.2",
    "react-dom": "^16.2.0",
    "react-equalizer": "^1.3.0",
    "react-pure-modal": "^1.5.1",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.0",
    "react-slick": "^0.18.1",
    "react-swipeable": "^4.2.1",
    "slick-carousel": "^1.8.1",
    "universal-cookie": "^2.1.2"
  },
  "scripts": {
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "mv public/index-build.html public/index.html && npm run build-css && react-scripts build && node bin/postbuild.js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive"
  }
}
