{
  "name": "website-client",
  "version": "1.1.9",
  "private": true,
  "dependencies": {
    "bootstrap-4-grid": "^2.3.0",
    "html-react-parser": "^0.4.3",
    "lodash.chunk": "^4.2.0",
    "lodash.startswith": "^4.2.1",
    "node-sass-chokidar": "0.0.3",
    "normalize.css": "^7.0.0",
    "npm-run-all": "^4.1.2",
    "query-string": "^5.1.1",
    "react": "^16.3.1",
    "react-cookie": "^2.1.4",
    "react-document-meta": "^2.1.2",
    "react-dom": "^16.2.0",
    "react-equalizer": "^1.3.0",
    "react-pure-modal": "^1.5.1",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.0",
    "react-slick": "^0.18.1",
    "react-swipeable": "^4.2.1",
    "request": "^2.85.0",
    "request-promise-native": "^1.0.5",
    "slick-carousel": "^1.8.1",
    "universal-cookie": "^2.1.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/registerServiceWorker.js",
      "!src/components/shelves/GuidanceShelf.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 60,
        "functions": 60,
        "lines": 60,
        "statements": 60
      }
    }
  },
  "scripts": {
    "start-js": "react-app-rewired start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "mv public/index-build.html public/index.html && npm run build-css && react-app-rewired build && node bin/postbuild.js",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive"
  },
  "devDependencies": {
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint-plugin-react": "^7.8.2",
    "react-app-rewire-eslint": "^0.2.3",
    "react-app-rewired": "^1.5.2"
  }
}
